<p id="notice"><%= notice %></p>

<h1>Genes</h1>

<table>
  <thead>
    <tr>
      <th>Specimen sample</th>
      <th>Gene name</th>
      <th>Dna sequence</th>
      <th>Protein name</th>
      <th>Amino acid sequence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @genes.each do |gene| %>
      <tr>
        <td><%= gene.specimen_sample %></td>
        <td><%= gene.gene_name %></td>
        <td><%= gene.dna_sequence %></td>
        <td><%= gene.protein_name %></td>
        <td><%= gene.amino_acid_sequence %></td>
        <td><%= link_to 'Show', gene %></td>
        <td><%= link_to 'Edit', edit_gene_path(gene) %></td>
        <td><%= link_to 'Destroy', gene, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gene', new_gene_path %>
